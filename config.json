{
    "app": {
        "name": "MyProject",
        "version": "1.0.0"
    },
    "logging": {
        "level": "INFO",
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "file": {
            "enabled": true,
            "path": "logs/app.log",
            "max_size": 10000000,
            "backup_count": 5
        },
        "console": {
            "enabled": true,
            "colored": true
        }
    }
} 